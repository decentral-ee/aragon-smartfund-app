{
  "name": "aragon-smartfund-app",
  "version": "0.0.1",
  "description": "",
  "dependencies": {
    "@aragon/os": "^4.2.0",
    "@aragon/apps-shared-minime": "^1.0.1",
    "@aragon/apps-token-manager": "^2.1.0",
    "@aragon/apps-voting": "^2.1.0",
    "@aragon/apps-vault": "^4.1.0",
    "@aragon/apps-finance": "^3.0.0",
    "openzeppelin-solidity": "2.3.0"
  },
  "devDependencies": {
    "@aragon/cli": "^5.9.3",
    "@aragon/test-helpers": "^2.0.0",
    "babel-eslint": "^10.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-standard": "^4.0.0",
    "ethlint": "^1.2.4",
    "prettier": "^1.18.2",
    "solidity-coverage": "^0.5.11",
    "openzeppelin-test-helpers": "^0.4.0"
  },
  "scripts": {
    "prepare": "cd app && npm install && cd ..",
    "devchain": "aragon devchain --port 38401",
    "dev:app": "cd app && APP_HTTP_PORT=8401 npm start && cd ..",
    "dev:aragon": "npm run aragon:run:http:local",
    "aragon:run": "REACT_APP_DEFAULT_ETH_NODE=ws://localhost:38401 aragon run",
    "aragon:run:ipfs": "npm run aragon:run -- --client-port 8400",
    "aragon:run:http": "npm run aragon:run -- --client-port 8400 --http localhost:8401 --http-served-from ./dist",
    "aragon:run:ipfs:local": "npm run aragon:run:ipfs -- --template LocalTemplate --template-init @ARAGON_ENS",
    "aragon:run:http:local": "npm run aragon:run:http -- --template LocalTemplate --template-init @ARAGON_ENS",
    "test": "cross-env TRUFFLE_TEST=true npm run ganache-cli:test",
    "compile": "aragon contracts compile",
    "build": "cd app && npm run build && cd ..",
    "publish:patch": "aragon apm publish patch",
    "publish:minor": "aragon apm publish minor",
    "publish:major": "aragon apm publish major",
    "versions": "aragon apm versions",
    "lint": "eslint . & solium --dir ./contracts",
    "lint:fix": "eslint . --fix & solium --dir ./contracts --fix",
    "coverage": "cross-env SOLIDITY_COVERAGE=true npm run ganache-cli:test",
    "ganache-cli:test": "sh ./node_modules/@aragon/test-helpers/ganache-cli.sh"
  }
}
